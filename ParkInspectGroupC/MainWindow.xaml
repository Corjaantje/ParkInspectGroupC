<Window x:Class="ParkInspectGroupC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParkInspectGroupC"
		xmlns:ts="clr-namespace:ParkInspectGroupC.View"
		xmlns:this="clr-namespace:ParkInspectGroupC.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
		DataContext="{Binding Main, Source={StaticResource Locator}}">
	<Grid>
		<ts:SettingsView />
		<ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="260,72,0,0" VerticalAlignment="Top" Width="120"/>
		<Label x:Name="label" Content="{Binding Test}" HorizontalAlignment="Left" Margin="224,163,0,0" VerticalAlignment="Top"/>
		<Calendar HorizontalAlignment="Left" Margin="81,141,0,0" VerticalAlignment="Top"/>
		<Slider x:Name="slider" HorizontalAlignment="Left" Margin="101,111,0,0" VerticalAlignment="Top" Width="159"/>
		<ToolBar x:Name="toolBar" HorizontalAlignment="Left" Margin="22,60,0,0" VerticalAlignment="Top" Width="238"/>
		<Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="352,148,0,0" VerticalAlignment="Top" Width="75"/>
	</Grid>
</Window>
