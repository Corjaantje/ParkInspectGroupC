<Window x:Class="ParkInspectGroupC.View.MapView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:WindowsPresentation="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation"
        Title="MapView" Width="540" Height="399"
        DataContext="{Binding Map, Source={StaticResource Locator}}">

    <Grid>
        <WindowsPresentation:GMapControl x:Name="gmap" HorizontalAlignment="Left" Height="299" Margin="10,10,0,0" VerticalAlignment="Top" Width="512" MaxZoom="18" MinZoom="2" Zoom="7"/>
        <Button Content="Toon alle inspecties" HorizontalAlignment="Left" Margin="10,314,0,0" VerticalAlignment="Top" Width="157" Height="44" Command="{Binding Path=ShowInspectionsCommand}"/>
    </Grid>
</Window>
