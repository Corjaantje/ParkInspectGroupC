<Window x:Class="ParkInspectGroupC.View.Map"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:WindowsPresentation="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation"
       DataContext="{Binding Map, Source={StaticResource Locator}}"
        Title="Map" Height="300" Width="300">
    <Grid>
        <WindowsPresentation:GMapControl x:Name="gmap" HorizontalAlignment="Left" Height="299" Margin="10,10,0,0" VerticalAlignment="Top" Width="497" MaxZoom="18" MinZoom="2" Zoom="5" MouseDown="OnMouseDown" MouseUp="OnMouseUp"/>
    </Grid>
</Window>
